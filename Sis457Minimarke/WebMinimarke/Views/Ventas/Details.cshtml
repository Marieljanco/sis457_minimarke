@model WebMinimarke.Models.Ventum

@{
    ViewData["Title"] = "Detalle Venta";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Detalle Venta</h1>

<div>
    <h4>Ventas</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">Transaccion</dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Transaccion)
        </dd>
        <dt class="col-sm-2">Fecha</dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Fecha)
        </dd>
        <dt class="col-sm-2">IdClienteNavigation</dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdClienteNavigation.Id)
        </dd>
        <dt class="col-sm-2">IdEmpleadoNavigation</dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdEmpleadoNavigation.Id)
        </dd>
        <dt class="col-sm-2">IdProductoNavigation</dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdProductoNavigation.Id)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id">Editar</a> |
    <a asp-action="Index">Volver a la lista</a>
</div>
