@model WebMinimarke.Models.Inventario

@{
    ViewData["Title"] = "Eliminar inventario";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Eliminar</h1>

<h3>¿Seguro de querer eliminar el inventario?</h3>
<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">Id Proveedor</dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdProveedor)
        </dd>
        <dt class="col-sm-2">Cantidad Stock</dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.CantidadStock)
        </dd>
        <dt class="col-sm-2">Fecha Ultima Reposicion</dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.FechaUltimaRepocision)
        </dd>
        <dt class="col-sm-2">IdEmpleado Navigation</dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdEmpleadoNavigation.Id)
        </dd>
        <dt class="col-sm-2">IdProducto Navigation</dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdProductoNavigation.Id)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a asp-action="Index">Volver a la Lista</a>
    </form>
</div>
